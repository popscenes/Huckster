  <!-- Page Print Direction -->
  <xs:simpleType name="page-dir">
    <xs:restriction base="xs:string">
      <xs:enumeration value="left_to_right">
        <xs:annotation>
          <xs:documentation>
            Left to right
            (No rotation. Data is printed from the top left corner to
            the right.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="bottom_to_top">
        <xs:annotation>
          <xs:documentation>
            Bottom to top
            (Counterclockwise rotation by 90 degrees.
            Data is printed from the bottom left corner to the top.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="right_to_left">
        <xs:annotation>
          <xs:documentation>
            Right to left
            (Rotation by 180 degrees. Data is printed from the bottom
            right corner to the left.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="top_to_bottom">
        <xs:annotation>
          <xs:documentation>
            Top to bottom
            (Clockwise rotation by 90 degrees.
            Data is printed from the top right corner to the bottom.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <!-- Cut Type -->
  <xs:simpleType name="cut-type">
    <xs:restriction base="xs:string">
      <xs:enumeration value="no_feed">
        <xs:annotation>
          <xs:documentation>
            Cut without feeding
            (The paper is cut without being fed.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="feed">
        <xs:annotation>
          <xs:documentation>
            Feed cut
            (The paper is fed to the cut position and then is cut.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="reserve">
        <xs:annotation>
          <xs:documentation>
            Cut reservation
            (Printing continues until the cut position is reached, at
            which the paper is cut.)
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>


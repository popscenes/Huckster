  <xs:simpleType name="text-scale">
    <xs:restriction base="xs:unsignedByte">
      <xs:maxInclusive value="8" />
      <xs:minInclusive value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Print Text -->
  <xs:complexType name="text">
    <xs:annotation>
      <xs:documentation>
        Prints a character string. Also, configure the string-related settings such as style, print position, and line feed
        space.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="lang" type="xs:language">
          <xs:annotation>
            <xs:documentation>
              Specifies the target language.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="align" type="tns:align">
          <xs:annotation>
            <xs:documentation>
              Specifies the alignment.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="font" type="tns:font">
          <xs:annotation>
            <xs:documentation>
              Specifies the character font.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="linespc" type="xs:unsignedByte">
          <xs:annotation>
            <xs:documentation>
              Specifies the per-line paper feed amount in dots.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dw" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets the double-sized width.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="dh" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets the double-sized height.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotate" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets rotated printing of text.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="reverse" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets the inversion of black and white parts of characters.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ul" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets underlining.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="em" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets emphasized printing.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="color" type="tns:color">
          <xs:annotation>
            <xs:documentation>
              Specifies the character color.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="width" type="tns:text-scale">
          <xs:annotation>
            <xs:documentation>
              Specifies the horizontal scale.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="height" type="tns:text-scale">
          <xs:annotation>
            <xs:documentation>
              Specifies the vertical scale.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="smooth" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Sets smoothing. When smoothing is enabled, the print quality of characters will be
              improved.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="x" type="xs:unsignedShort">
          <xs:annotation>
            <xs:documentation>
              Specifies the print start position of the text in dots.
              In page mode, this specification is the same as the specification for the horizontal print
              position by the position element, but the print position in the horizontal direction only
              will be moved.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="y" type="xs:unsignedShort">
          <xs:annotation>
            <xs:documentation>
              In page mode, this specification is the same as the specification for the vertical print
              position by the position element, but the print position in the vertical direction only
              will be moved. (in Ver.3.0 and later)
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!-- Feed Paper -->
  <xs:complexType name="feed">
    <xs:annotation>
      <xs:documentation>
        Feeds paper. Specifies the paper feed amount in dots or in lines. When no paper feed amount is specified,
        performs single-line paper feed (line feed). In addition, the per-line line feed space can be set.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="line" type="xs:unsignedByte">
      <xs:annotation>
        <xs:documentation>
          Specifies the paper feed amount in lines.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="unit" type="xs:unsignedByte">
      <xs:annotation>
        <xs:documentation>
          Specifies the paper feed amount in dots.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="linespc" type="xs:unsignedByte">
      <xs:annotation>
        <xs:documentation>
          Specifies the per-line paper feed amount in dots.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="pos" type="tns:feed-pos">
      <xs:annotation>
        <xs:documentation>
          It specifies paper feed position of label paper/black mark paper. (in Ver.2.1 and later)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Print Image (Raster Bit Image) -->
  <xs:complexType name="image">
    <xs:annotation>
      <xs:documentation>
        Print a raster graphic bit-image. (Data type xs:base64Binary)
        Raster graphics refer to data generated by horizontally scanning the pixels of an image from the top left
        corner of that image as the origin point.
        Data is generated based on 1 bit per pixel for two-tone images, and 4 bits per pixel for 16-tone images,
        starting with the upper bits of byte data. And zero-padding is performed so that scan data per line can be
        treated on a byte basis.
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:base64Binary">
        <xs:attribute name="width" type="xs:unsignedShort" use="required">
          <xs:annotation>
            <xs:documentation>
              Specifies the image width in dots.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="height" type="xs:unsignedShort" use="required">
          <xs:annotation>
            <xs:documentation>
              Specifies the image height in dots.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="align" type="tns:align">
          <xs:annotation>
            <xs:documentation>
              Specifies the print position.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="color" type="tns:color" default="color_1">
          <xs:annotation>
            <xs:documentation>
              Specifies the character color.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="mode" type="tns:image-mode" default="mono">
          <xs:annotation>
            <xs:documentation>
              Specifies the color mode.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!-- Print Logo (NV Bit Image) -->
  <xs:complexType name="logo">
    <xs:annotation>
      <xs:documentation>
        Prints a logo registered in the NV memory of the printer.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="key1" type="xs:unsignedByte" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the value of the key code 1 set at the time of NV logo registration.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key2" type="xs:unsignedByte" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the value of the key code 2 set at the time of NV logo registration. Be sure to
          specify this value.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="align" type="tns:align">
      <xs:annotation>
        <xs:documentation>
          Specifies the print position.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="rotate" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>
          Specifies the rotate printing.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Print Bar Code -->
  <xs:complexType name="barcode">
    <xs:annotation>
      <xs:documentation>
        Prints a barcode.
        Specifies the barcode data using a character string. (Data type xs:string)
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type" type="tns:barcode-type" use="required">
          <xs:annotation>
            <xs:documentation>
              Specifies the barcode type.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="align" type="tns:align">
          <xs:annotation>
            <xs:documentation>
              Specifies the print position.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="hri" type="tns:barcode-hri">
          <xs:annotation>
            <xs:documentation>
              Specifies the HRI position.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="font" type="tns:font">
          <xs:annotation>
            <xs:documentation>
              Specifies the HRI font.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="width" type="xs:unsignedByte">
          <xs:annotation>
            <xs:documentation>
              Specifies the width of each module in dots.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="height" type="xs:unsignedByte">
          <xs:annotation>
            <xs:documentation>
              Specifies the barcode height in dots.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotate" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Specifies the rotate printing.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!-- Print 2-D Symbol -->
  <xs:complexType name="symbol">
    <xs:annotation>
      <xs:documentation>
        Prints a two-dimensional symbol.
        Specifies the two-dimensional symbol using a character string. (Data type xs:string)
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type" type="tns:symbol-type" use="required">
          <xs:annotation>
            <xs:documentation>
              Specifies the two-dimensional symbol type.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="align" type="tns:align">
          <xs:annotation>
            <xs:documentation>
              Specifies the print position.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="level" type="tns:symbol-level">
          <xs:annotation>
            <xs:documentation>
              Specifies the error correction level.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="width" type="xs:unsignedByte">
          <xs:annotation>
            <xs:documentation>
              Specifies the width of each module in dots.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="height" type="xs:unsignedByte">
          <xs:annotation>
            <xs:documentation>
              Specifies the height of each module.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="size" type="xs:unsignedShort">
          <xs:annotation>
            <xs:documentation>
              Specifies the two‐dimensional symbol maximum size.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotate" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>
              Specifies the rotate printing.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <!-- Draw Horizontal Line -->
  <xs:complexType name="hline">
    <xs:annotation>
      <xs:documentation>
        Draws horizontal lines.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x1" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the draw start position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="x2" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the draw end position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="style" type="tns:line-style" default="thin">
      <xs:annotation>
        <xs:documentation>
          Specifies the line type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Draw Vertical Line -->
  <xs:complexType name="vline-begin">
    <xs:annotation>
      <xs:documentation>
        Starts to draw vertical lines.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the start position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="style" type="tns:line-style" default="thin">
      <xs:annotation>
        <xs:documentation>
          Specifies the line type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- End Vertical Line -->
  <xs:complexType name="vline-end">
    <xs:annotation>
      <xs:documentation>
        Ends to draw vertical lines.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the end position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="style" type="tns:line-style" default="thin">
      <xs:annotation>
        <xs:documentation>
          Specifies the line type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Page Mode -->
  <xs:complexType name="page">
    <xs:annotation>
      <xs:documentation>
        Changes the mode from standard mode to page mode.
      </xs:documentation>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element name="feed" type="tns:feed" />
      <xs:element name="text" type="tns:text" />
      <xs:element name="image" type="tns:image" />
      <xs:element name="logo" type="tns:logo" />
      <xs:element name="barcode" type="tns:barcode" />
      <xs:element name="symbol" type="tns:symbol" />
      <xs:element name="area" type="tns:area" />
      <xs:element name="direction" type="tns:direction" />
      <xs:element name="position" type="tns:position" />
      <xs:element name="line" type="tns:line" />
      <xs:element name="rectangle" type="tns:rectangle" />
      <xs:element name="command" type="tns:command" />
    </xs:choice>
  </xs:complexType>

  <!-- Set Print Area -->
  <xs:complexType name="area">
    <xs:annotation>
      <xs:documentation>
        Specifies the page mode print area.
        Specifies the origin point, width, and height based on the absolute origin point to set the print area. The
        absolute origin point is a dot diagonally above the top left corner of the printable area.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the origin of the horizontal axis (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the origin of the vertical axis (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="width" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the width of the print area (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="height" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the height of the print area (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Set Print Direction -->
  <xs:complexType name="direction">
    <xs:annotation>
      <xs:documentation>
        Specifies the page mode print direction.
        Specifies the print direction and rotates the print area. Following the rotation of the print area, the start point
        of the print area moves.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="dir" type="tns:page-dir" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the rotation direction.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Set Print Position -->
  <xs:complexType name="position">
    <xs:annotation>
      <xs:documentation>
        Specifies the page mode print position.
        Specifies the print position based on the start point of the print area. The start point of the print area moves
        by following the rotation of the print area.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the origin of the horizontal axis (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the origin of the vertical axis (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Draw Line -->
  <xs:complexType name="line">
    <xs:annotation>
      <xs:documentation>
        Draws a line in page mode.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x1" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing start position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y1" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing start position of the vertical line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="x2" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing end position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y2" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing end position of the vertical line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="style" type="tns:line-style" default="thin">
      <xs:annotation>
        <xs:documentation>
          Specifies the line type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Draw Rectangle -->
  <xs:complexType name="rectangle">
    <xs:annotation>
      <xs:documentation>
        Draws a rectangle in page mode.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="x1" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing start position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y1" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing start position of the vertical line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="x2" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing end position of the horizontal line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="y2" type="xs:unsignedShort" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawing end position of the vertical line (in dots).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="style" type="tns:line-style" default="thin">
      <xs:annotation>
        <xs:documentation>
          Specifies the line type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Cut Paper -->
  <xs:complexType name="cut">
    <xs:annotation>
      <xs:documentation>
        Sets paper cut.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="type" type="tns:cut-type" default="feed">
      <xs:annotation>
        <xs:documentation>
          Specifies the paper cut type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Generate Pulse -->
  <xs:complexType name="pulse">
    <xs:annotation>
      <xs:documentation>
        Outputs signals to the drawer kick connector.
        Depending on the model, a buzzer can be sounded.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="drawer" type="tns:pulse-drawer" default="drawer_1">
      <xs:annotation>
        <xs:documentation>
          Specifies the drawer kick connector.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="time" type="tns:pulse-time" default="pulse_100">
      <xs:annotation>
        <xs:documentation>
          Specifies the ON time of the drawer kick signal.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Sound Buzzer -->
  <xs:complexType name="sound">
    <xs:annotation>
      <xs:documentation>
        Sounds a buzzer.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="pattern" type="tns:sound-pattern" default="pattern_a">
      <xs:annotation>
        <xs:documentation>
          Specifies the buzzer pattern.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="repeat" type="xs:unsignedByte" default="1">
      <xs:annotation>
        <xs:documentation>
          Specifies the number of repeats.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="cycle" type="xs:unsignedShort" default="1000">
      <xs:annotation>
        <xs:documentation>
          Specifies the buzzer sounding cycle. (in units of milliseconds) (in Ver.2.2 and later)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Set Paper Layout -->
  <xs:complexType name="layout">
    <xs:annotation>
      <xs:documentation>
        Specifies paper layout. (in Ver.2.2 and later)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="type" type="tns:layout-type" use="required">
      <xs:annotation>
        <xs:documentation>
          Specifies the paper type.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="width" type="xs:unsignedShort" default="580">
      <xs:annotation>
        <xs:documentation>
          Specifies paper width (in units of 0.1mm).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="height" type="xs:unsignedShort" default="0">
      <xs:annotation>
        <xs:documentation>
          Specifies paper height (in units of 0.1mm).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="margin-top" type="xs:short" default="0">
      <xs:annotation>
        <xs:documentation>
          Specifies top margin (in units of 0.1mm).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="margin-bottom" type="xs:short" default="0">
      <xs:annotation>
        <xs:documentation>
          Specifies bottom margin (in units of 0.1mm).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="offset-cut" type="xs:short" default="0">
      <xs:annotation>
        <xs:documentation>
          Specifies cut position (in units of 0.1mm).
          In case of die cut label paper, it is a distance from the bottom of label.
          When a paper has black mark, it is a distance from the beginning of black mark.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="offset-label" type="xs:short" default="0">
      <xs:annotation>
        <xs:documentation>
          Specifies label bottom position (in units of 0.1mm).
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Recover From Error -->
  <xs:complexType name="recovery">
    <xs:annotation>
      <xs:documentation>
        If a printer recoverable error is generated, execute this function after removing the error cause,
        and the TM printer recovers from the error. (in Ver.3.0 and later)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
  </xs:complexType>

  <!-- Reset Printer -->
  <xs:complexType name="reset">
    <xs:annotation>
      <xs:documentation>
        Resets the printer. (in Ver.3.0 and later)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
  </xs:complexType>

  <!-- Send Command -->
  <xs:complexType name="command">
    <xs:annotation>
      <xs:documentation>
        Insert the ESC/POS command.
        ESC/POS command is specified in hexadecimal encoding. (data type xs:hexBinary)
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:hexBinary" />
    </xs:simpleContent>
  </xs:complexType>

  <!-- ePOS-Print Header -->
  <xs:complexType name="parameter">
    <xs:annotation>
      <xs:documentation>
        A SOAP Header to be sent from a host to a printer. (in Ver.4.1 and later)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="devid" type="xs:string" minOccurs="0" />
      <xs:element name="timeout" type="xs:string" minOccurs="0" />
      <xs:element name="printjobid" type="xs:string" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="parameter" type="tns:parameter" />

  <!-- ePOS-Print Request -->
  <xs:complexType name="epos-print">
    <xs:annotation>
      <xs:documentation>
        An XML document to be sent from a host to a printer.
        This document requests the printer to execute the specified function and performs initialization when
        printing starts.
      </xs:documentation>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element name="feed" type="tns:feed" />
      <xs:element name="text" type="tns:text" />
      <xs:element name="image" type="tns:image" />
      <xs:element name="logo" type="tns:logo" />
      <xs:element name="barcode" type="tns:barcode" />
      <xs:element name="symbol" type="tns:symbol" />
      <xs:element name="hline" type="tns:hline" />
      <xs:element name="vline-begin" type="tns:vline-begin" />
      <xs:element name="vline-end" type="tns:vline-end" />
      <xs:element name="page" type="tns:page" />
      <xs:element name="cut" type="tns:cut" />
      <xs:element name="pulse" type="tns:pulse" />
      <xs:element name="sound" type="tns:sound" />
      <xs:element name="layout" type="tns:layout" />
      <xs:element name="recovery" type="tns:recovery" />
      <xs:element name="reset" type="tns:reset" />
      <xs:element name="command" type="tns:command" />
    </xs:choice>
    <xs:attribute name="force" type="xs:boolean" default="false">
      <xs:annotation>
        <xs:documentation>
          Sets the forced transmission. (in Ver.3.0 and later)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="epos-print" type="tns:epos-print" />

  <!-- ePOS-Print Response -->
  <xs:complexType name="response">
    <xs:annotation>
      <xs:documentation>
        An XML document, a printer returns to a host as a response.
        This document sends the printer status after command execution.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence />
    <xs:attribute name="success" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>
          Obtains the print result.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="code" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          Obtains the error code.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="status" type="xs:unsignedInt">
      <xs:annotation>
        <xs:documentation>
          Obtains the printer status.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="battery" type="xs:unsignedInt">
      <xs:annotation>
        <xs:documentation>
          The battery status of the printer is obtained.
          0 is indicated when the model doesn't have a battery installed. (in Ver.2.2 and later)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="response" type="tns:response" />

</xs:schema>
